{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "config.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/[...all].js" },
    { "src": "/favicon.(ico|png)", "status": 204 },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}


